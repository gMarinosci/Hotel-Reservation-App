<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="460.0" prefWidth="695.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="se.scandium.hotelproject.controller.fxml.ResetPasswordScreenController">
    <children>
        <AnchorPane layoutY="-6.0" prefHeight="63.0" prefWidth="700.0" style="-fx-background-color: #424242;">
            <children>
                <Label alignment="CENTER" layoutX="207.0" layoutY="11.0" prefHeight="41.0" prefWidth="308.0" text="Reset Password" textFill="WHITE">
                    <font>
                        <Font size="24.0" />
                    </font>
                </Label>
            </children>
        </AnchorPane>

        <Text fx:id="usernameText" layoutX="194.0" layoutY="119.0" style="-fx-font-size: 14px;-fx-font-weight: bold;"/>

        <JFXTextField fx:id="screenTitle" focusColor="#9a0007" layoutX="194.0" layoutY="135.0" prefHeight="34.0" prefWidth="329.0" promptText="Screen Title" unFocusColor="#424242">
            <font>
                <Font size="14.0" />
            </font>
        </JFXTextField>
        <JFXPasswordField fx:id="oldPassword" focusColor="#9a0007" layoutX="194.0" layoutY="213.0" prefHeight="34.0" prefWidth="329.0" promptText="Old Password" unFocusColor="#424242">
            <font>
                <Font size="14.0" />
            </font>
        </JFXPasswordField>

        <JFXPasswordField fx:id="newPassword" focusColor="#9a0007" layoutX="194.0" layoutY="256.0" prefHeight="34.0" prefWidth="329.0" promptText="New Password" unFocusColor="#424242">
            <font>
                <Font size="14.0" />
            </font>
        </JFXPasswordField>

        <JFXPasswordField fx:id="reNewPassword" focusColor="#9a0007" layoutX="194.0" layoutY="297.0" prefHeight="34.0" prefWidth="329.0" promptText="Repeat New Password" unFocusColor="#424242">
            <font>
                <Font size="14.0" />
            </font>
        </JFXPasswordField>
        <JFXButton fx:id="submit" layoutX="194.0" layoutY="355.0" prefHeight="41.0" prefWidth="329.0" style="-fx-background-color: #0bc76f;-fx-font-weight: bold;-fx-font-size: 14px;" text="Submit" textFill="#f8f7f7" />

    </children>
</AnchorPane>
